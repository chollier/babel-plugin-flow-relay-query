{
  "name": "babel-plugin-flow-relay-query",
  "version": "0.7.1",
  "description": "Babel plugin which converts Flow types into Relay fragments",
  "keywords": [
    "babel-plugin", "relay", "flow"
  ],
  "license": "MIT",
  "repository": "guymers/babel-plugin-flow-relay-query",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],

  "scripts": {
    "update-schema": "babel-node test/data/updateSchema.js",

    "check": "npm run check:lint && npm run check:types",
    "check:lint": "eslint --ext .js .",
    "check:types": "flow check",

    "build": "rm -rf ./lib && babel src --out-dir lib --copy-files",
    "do-publish": "npm run check && npm run build && npm test && npm publish",
    "test": "mocha test/fixture",
    "coverage": "rm -rf coverage && babel-node node_modules/istanbul/lib/cli cover _mocha -- test/fixture"
  },

  "dependencies": {
    "babel-core": "^6.6.4",
    "babel-traverse": "^6.5.0",
    "babel-plugin-syntax-async-functions": "^6.5.0",
    "babel-plugin-syntax-class-constructor-call": "^6.5.0",
    "babel-plugin-syntax-class-properties": "^6.5.0",
    "babel-plugin-syntax-decorators": "^6.5.0",
    "babel-plugin-syntax-do-expressions": "^6.5.0",
    "babel-plugin-syntax-exponentiation-operator": "^6.5.0",
    "babel-plugin-syntax-export-extensions": "^6.5.0",
    "babel-plugin-syntax-flow": "^6.5.0",
    "babel-plugin-syntax-function-bind": "^6.5.0",
    "babel-plugin-syntax-jsx": "^6.5.0",
    "babel-plugin-syntax-object-rest-spread": "^6.5.0",
    "babel-plugin-syntax-trailing-function-commas": "^6.5.0",

    "babel-relay-plugin": "0.8.0",
    "graphql": "^0.4.18"
  },

  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",

    "mocha": "2.4.5",
    "istanbul": "1.0.0-alpha.2",
    "glob": "7.0.3",
    "react": "15.0.1",
    "react-dom": "15.0.1",
    "react-relay": "0.8.0",

    "babel-eslint": "6.0.2",
    "eslint": "2.8.0",
    "eslint-plugin-flow-vars": "0.3.0",
    "eslint-plugin-react": "4.3.0",
    "eslint-plugin-jsx-a11y": "0.6.2",
    "eslint-config-airbnb": "7.0.0",
    "flow-bin": "0.23.0"
  }
}
