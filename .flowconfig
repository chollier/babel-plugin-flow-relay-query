[include]
./node_modules/fbjs/lib
./node_modules/react
./node_modules/react-static-container/lib

[ignore]
.*/babel-plugin-flow-relay-query/coverage/.*
.*/babel-plugin-flow-relay-query/lib/.*
.*/babel-plugin-flow-relay-query/test/.*

.*/node_modules/fbjs/flow/include/.*
.*/node_modules/react/.*

// people who package their tests..
.*/node_modules/.*/test/.*

// unparsable json
.*/node_modules/**/.*.json

// exclude files that will never be imported
.*/node_modules/babel-.*
.*/node_modules/babylon/.*
.*/node_modules/eslint.*/.*

[libs]
./flow/lib
./node_modules/fbjs/flow/lib

[options]
module.system=haste
munge_underscores=true

suppress_type=$FlowIssue
